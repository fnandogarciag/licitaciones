{
  "name": "licitaciones",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "check": "node -r dotenv/config ./scripts/check-env.js && node -r dotenv/config ./scripts/check-db.js",
    "dev": "npm run check && next dev",
    "build": "tsc --project tsconfig.build.json && node ./scripts/ensure-dist-root-datasource.js && next build",
    "start": "next start",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "seed": "node ./src/scripts/seed.js"
  },
  "dependencies": {
    "class-validator": "^0.14.2",
    "js-cookie": "^3.0.5",
    "next": "16.0.1",
    "pg": "^8.16.3",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.27"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20.19.24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^16.6.1",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "eslint-config-prettier": "^10.1.8",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
